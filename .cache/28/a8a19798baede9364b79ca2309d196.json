column":1},"source":"scripts/modules/filter/list.js","original":{"line":4,"column":0}},{"generated":{"line":73,"column":15},"source":"scripts/modules/filter/list.js","original":{"line":71,"column":15},"name":"List"}],"sources":{"scripts/modules/filter/list.js":"import React, { useState, useEffect } from 'react';\nimport Listing from './listing.js';\n\nconst List = ({loadItems, setLoadItems, isLoading, listings}) => {\n\n    let maxItems = 6;\n    const loadMoreBtn = (e) => {\n      e.preventDefault();\n      \n      setLoadItems(loadItems + maxItems);\n      \n    }\n\n    useEffect(() => {\n      setLoadItems(6);\n    }, []);\n\n    let button;\n    if (listings.length >= loadItems) {\n\n      button = (\n        <a href=\"#\" className=\"listing-cards__load-btn\" onClick={loadMoreBtn}>\n          Load More\n        </a>\n      );\n\n    } else {\n      button = \"\";\n    }\n\n    if (!isLoading && listings.length == 0) {\n      return (\n        <>\n        <div className=\"col-md-9\">\n          <div className=\"course-intro text-primary text-center mb-3\">\n            <header className=\"entry-header\">\n              <h1 className=\"entry-header\">Course reviews</h1>\n              <p>Select from individual courses or let us build your own curriculum.</p>\n            </header>\n          </div>\n          <p><strong>Oops...</strong></p>\n          <p><strong>Sorry! We found no results. Try broadening your search.</strong></p>\n        </div>\n        </>\n      );\n    } else {\n      return (\n        <>\n        <div className=\"col-md-9\">\n          <div className=\"course-intro text-primary text-center mb-3\">\n            <header className=\"entry-header\">\n              <h1 className=\"entry-header\">Course reviews</h1>\n              <p>Select from individual courses or let us build your own curriculum.</p>\n            </header>\n          </div>\n            {\n              !isLoading && listings.length > 0 && (\n                  listings.slice(0, loadItems).map((list, index) => (\n                    <Listing key={index} list={list}/>\n                  )\n                )\n              )\n            }\n          {button}\n        </div>\n        </>\n      );\n    }\n};\n\nexport default List;"},"lineCount":null}},"error":null,"hash":"65eb8cd38a4af583fb5ed215af23d2e9","cacheData":{"env":{}}}